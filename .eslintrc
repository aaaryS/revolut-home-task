---
parser: babel-eslint
parserOptions:
  ecmaVersion: 6
  sourceType: module
  ecmaFeatures:
    blockBindings: true
    jsx: true
env:
  browser: true
  es6: true
  node: true
  jasmine: true
extends:
  eslint:recommended
plugins:
  - react
rules:
  no-console: 1
  react/jsx-uses-react: 1
  react/jsx-uses-vars: 1
